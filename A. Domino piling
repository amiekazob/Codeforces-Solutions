#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
int main() {
  ios_base::sync_with_stdio(0);cin.tie(0);
  int m , n; cin >> m >> n;
  int res1 = (m / 2) * (n / 1); 
  if (m % 2 == 1 ) res1 += (n / 2);
  int res2 = (m / 1) * (n / 2);
  if (n % 2 == 1 ) res2 += (m / 2);
  cout << max(res1, res2);   
  return 0;
}

